    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@prisma/client": "6.11.1",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
